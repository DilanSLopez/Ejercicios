Algoritmo porcentaje
	Definir hombres,mujeres,total Como Real;
	Escribir "ingresa el total de mujeres";
	leer mujeres;
	Escribir "ingresa el total de hombre";
	Leer hombres;
	total = mujeres + hombres;
	Escribir "el total de alumnos en el grupo es:",total;
	Escribir "el porcentaje de mujeres es:", (mujeres/total)*100,"%";
	Escribir "el porcentaje de hombres es:",(hombres/total)*100,"%";
	
	
FinAlgoritmo
	